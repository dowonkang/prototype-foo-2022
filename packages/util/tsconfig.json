{
  "compilerOptions": {
    "target": "ESNEXT",
    "lib": ["DOM", "ESNEXT"],
    "module": "ESNEXT",
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "declarationDir": "./types",
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "removeComments": true
  },
  "exclude": ["./types/"]
}
